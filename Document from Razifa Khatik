    "# formatting the data\n",
    "cols=df.columns.tolist()\n",
    "cols.remove(\"Selling_Price\")\n",
    "owner=cols.index(\"Owner\")\n",
    "cols.insert(owner + 1,\"Selling_Price\")\n",
    "dff=df[cols]\n",
    "dff"
